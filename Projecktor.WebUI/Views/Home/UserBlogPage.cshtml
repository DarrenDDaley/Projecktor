@model IEnumerable<PostViewModel>

@{
    ViewBag.Title = "Projecktor External";
}

@section head
{
    @Scripts.Render("~/bundles/userpagescripts")
    <link href="~/Content/projecktorStyleExternal.css" rel="stylesheet" />
}

<h1 class="blog-header"><a class="blog-title" href="/">@Model.ToList()[0].Author.BlogTitle</a>
</h1>

@foreach (PostViewModel textPost in Model) {
    @Html.Partial("_PostModelExternal", textPost);
}

@Html.Partial("_PostAttachment")

