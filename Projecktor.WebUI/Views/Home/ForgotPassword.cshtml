@model ForgotPasswordViewModel

@{
    ViewBag.Title = "Projectktor | ForgotPassword";
}

<p>
    @using (var signupForm = Html.BeginForm("ForgotPassword", "Home"))
    {
        @Html.AntiForgeryToken()

        @Html.ValidationSummary() <br />
            @Html.TextBoxFor(m => m.Email, new { placeholder = "Email" }) <br />
            <input type="submit" value="Send" />
    }
</p>