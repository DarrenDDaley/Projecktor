@model IEnumerable<Projecktor.Domain.Entites.TextPost>

@{
    ViewBag.Title = "Projecktor";
}

<div>
    @Html.ActionLink("Text Post", "TextPost")
</div>

    @if (Model.Any())
    {
        foreach (var textPost in Model) {
            @Html.Partial("_TextPostPartial", textPost);
        }
    }
    else
    {
         <div class="posts">
            <p>Make a post !</p>
         </div>
    }