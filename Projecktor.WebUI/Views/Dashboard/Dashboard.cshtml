@model IEnumerable<Projecktor.Domain.Entites.TextPost>

@{
    ViewBag.Title = "Projecktor";
}

<h2>Dashboard</h2>

<div>
    @Html.ActionLink("Text Post", "TextPost")
</div>

    @if (Model.Any())
    {
        foreach (var post in Model)
        {
            <p>
            @post.Text
            </p>
        }
    }
    else {
        <p>Make a post !</p>
    }