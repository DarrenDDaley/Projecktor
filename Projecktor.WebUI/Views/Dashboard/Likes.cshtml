@model IEnumerable<PostViewModel>

@{
    ViewBag.Title = "Likes | Projecktor";
}

<div id="navigationBar">
    @Html.Partial("_NavigationBar")
</div>

<script src="~/Scripts/actionScripts.js"></script>
<script src="~/Scripts/infiniteScrollingScript.js"></script>
<script src="~/Scripts/infintiteScrollingDashboardLikes.js"></script>

@if (Model.Any() == true)
{
    foreach (PostViewModel textPost in Model) {
        @Html.Partial("_PostModelPartial", textPost);
    }
   @Html.Partial("_PostAttachment")
}
else
{
    <div class="posts">
        <p>Make a post !</p>
    </div>
}
